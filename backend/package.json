{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc && npm run copy-files",
    "copy-files": "node -e \"const fs=require('fs');const path=require('path');[{src:'src/db/migrations',dest:'dist/db/migrations'},{src:'migrations',dest:'dist/migrations'}].forEach(({src,dest})=>{if(fs.existsSync(src)){fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src).forEach(file=>{if(file.endsWith('.sql')){fs.copyFileSync(path.join(src,file),path.join(dest,file));console.log('Copied:',file);}});}});\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
